pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                git 'https://github.com/LinuxTute-GitHub/myfirstrepo.git'
            }
        }

        stage('Deploy') {
            steps {
                sh 'sh /home/jenkinsclient/myfirstscript.sh'
            }
        }
    }
}

